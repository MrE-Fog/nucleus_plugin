<?xml version="1.0" encoding="ISO-8859-1"?>
<template 
	type="ProjTempl" 
	version="1.0" 
	supplier="Mentor Graphics Corporation" 
	revision="1.0" 
	author="Mentor Graphics Corporation"
	copyright=""
	id="TLSClientDemo" 
	label="wolfSSL TLS Client Demo" 
	description="This sample application illustrates the use of Networking Middleware APIâ€™s to implement a TLS Client on the target."
	help="help.html">

	<process type="org.eclipse.cdt.managedbuilder.core.NewManagedProject">
		<simple name="name" value="$(projectName)" /> 
		<simple name="artifactExtension" value="exe" /> 
		<simple name="isCProject" value="true" /> 
	</process>

	<process type="org.eclipse.cdt.core.CreateSourceFolder">
		<simple name="projectName" value="$(projectName)"/>
		<simple name="path" value="src"/>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="files">
			<element>
				<simple name="source" value="tls_client.c"/>
				<simple name="target" value="src/$(projectName).c"/>
				<simple name="replaceable" value="true"/>
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.core.AddFiles">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="files">
			<element>
				<simple name="source" value="readme.txt"/>
				<simple name="target" value="readme.txt"/>
				<simple name="replaceable" value="true"/>
			</element>
		</complex-array>
	</process>

	<process type="org.eclipse.cdt.ui.OpenFiles">
		<simple name="projectName" value="$(projectName)"/>
		<complex-array name="files">
			<element>
				<simple name="target" value="readme.txt"/>
			</element>
		</complex-array>
	</process>

</template>

